@online{zefferman_Constraints_2022,
  title = {Constraints on Cooperation Shape Hierarchical vs Distributed Structure in Human Groups},
  author = {Zefferman, Matthew},
  date = {2022-04-19T20:38:22},
  eprinttype = {OSF Preprints},
  doi = {10.31219/osf.io/mkxbf},
  url = {https://osf.io/mkxbf/},
  urldate = {2023-03-14},
  abstract = {Cooperation is ubiquitous in human organizations and it is often maintained by monitoring and punishment institutions. However, there is much variation in how these institutions are structured. Some organizations are distributed, with diffuse monitoring and collective punishment. Others are hierarchical with centralized tree-like systems of monitoring and punishment. Why are some organizations distributed and others hierarchical? Models of cooperation typically assume either distributed or hierarchical structures, but they rarely consider why these different structures exist. I explore this question with a games-on-networks model and find that distributed organizations are favored when punishment is expensive, but monitoring is cheap. Hierarchical organizations are favored when punishment is cheap and, for these organizations, the cost of monitoring limits supervisors' spans of control. These findings suggest a path to increasing social and organizational complexity in human history and provide insight into why large-scale distributed organizations are possible when technologies, like the internet, allow for extremely efficient monitoring.},
  langid = {american},
  pubstate = {preprint},
  keywords = {Social and Behavioral Sciences}
}

@article{zefferman_Constraints_2023,
  title = {Constraints on Cooperation Shape Hierarchical versus Distributed Structure in Human Groups},
  author = {Zefferman, Matthew R.},
  date = {2023-01-20},
  journaltitle = {Scientific Reports},
  shortjournal = {Sci Rep},
  volume = {13},
  number = {1},
  pages = {1160},
  publisher = {{Nature Publishing Group}},
  issn = {2045-2322},
  doi = {10.1038/s41598-022-23454-9},
  url = {https://www.nature.com/articles/s41598-022-23454-9},
  urldate = {2023-02-09},
  abstract = {Some human groups are organized hierarchically and some are distributed. Both types of groups occur in economic, political, and military domains, but it is unclear why hierarchical organizations are favored in certain contexts and distributed organizations are favored in others. I propose that these different organizational structures can be explained by human groups having different constraints on their ability to foster cooperation within the group. Human within-group cooperation is often maintained by monitoring and punishment. In hierarchical groups, monitoring and punishment are organized into tree-like command-and-control structures with supervisors responsible for monitoring the cooperation of their subordinates and punishing non-cooperators. By contrast, in distributed groups, monitoring is diffuse and punishment is collective. I propose that the organization of cooperative human groups is constrained by the costs of monitoring and punishment. I formalize this hypothesis with a model where individuals in a group cooperate to produce public goods while embedded in a network of monitoring and punishment responsibilities. I show that, when punishment costs are high and monitoring costs are low, socially-optimal monitoring and punishment networks are distributed. The size of these distributed networks is constrained by monitoring costs. However, when punishment costs are low, socially-optimal networks are hierarchical. Monitoring costs do not constrain the size of hierarchical networks but determine how many levels of supervision are required to foster cooperation in the hierarchical group. These results may explain the increasingly large and hierarchical groups throughout much of human history. They also suggest that the recent emergence of large-scale distributed organizations has been possible because new technologies, like the internet, have made monitoring costs extremely low.},
  issue = {1},
  langid = {english},
  keywords = {Anthropology,Cultural evolution,Social evolution}
}

@article{zinkina_Circumscription_2016,
  title = {Circumscription {{Theory}} of the {{Origins}} of the {{State}}: {{A Cross-Cultural Re-Analysis}}},
  shorttitle = {Circumscription {{Theory}} of the {{Origins}} of the {{State}}},
  author = {Zinkina, Julia and Korotayev, Andrey and Andreev, Alexey},
  date = {2016},
  journaltitle = {Cliodynamics},
  volume = {7},
  number = {2},
  doi = {10.21237/C7clio7232817},
  url = {https://escholarship.org/uc/item/1sj9n878},
  urldate = {2022-11-15},
  abstract = {In the paper we express some doubts about one of the assumptions of Robert Carneiro’s model on state (and chiefdom) formation, namely the role of circumscription. In our opinion, the main flaw of Carneiro’s original theory of state formation is that it implicitly assumes that every community dreamt to conquer its neighboring communities. We test the presence of various types of warfare (such as conquest warfare, land acquisition warfare, and plunder warfare) in societies with different degrees of political centralization. Quantitative cross-cultural tests reveal a rather strong correlation between political complexity and the presence of conquest warfare suggesting that conquest warfare was virtually absent among independent communities. Newer works by Carneiro propose a model explaining how simple chiefdoms could appear in the absence of conquest warfare. This model also includes circumscription, but our analysis suggests that it is unnecessary.},
  langid = {english},
  file = {C\:\\Users\\cperret7\\Zotero\\storage\\KIY8C86J\\Zinkina et al. - 2016 - Circumscription Theory of the Origins of the State.pdf;C\:\\Users\\cperret7\\Zotero\\storage\\47V3J2ZQ\\1sj9n878.html}
}

