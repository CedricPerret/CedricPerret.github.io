@article{shen_ecology_2017,
  title = {The Ecology of Cooperative Breeding Behaviour},
  author = {Shen, Sheng-Feng and Emlen, Stephen T. and Koenig, Walter D. and Rubenstein, Dustin R.},
  date = {2017},
  journaltitle = {Ecology Letters},
