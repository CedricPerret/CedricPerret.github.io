@article{mcpeek_Evolution_1992,
  title = {The {{Evolution}} of {{Dispersal}} in {{Spatially}} and {{Temporally Varying Environments}}},
  author = {McPeek, Mark A. and Holt, Robert D.},
  date = {1992-12},
  journaltitle = {The American Naturalist},
