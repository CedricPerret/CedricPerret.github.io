@article{vehrencamp_model_1983,
  title = {A Model for the Evolution of Despotic versus Egalitarian Societies},
  author = {Vehrencamp, Sandra L.},
  date = {1983-08-01},
  journaltitle = {Animal Behaviour},
  shortjournal = {Animal Behaviour},
  volume = {31},
