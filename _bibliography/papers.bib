@article{smith_Ecological_2021,
  title = {Ecological Variation and Institutionalized Inequality in Hunter-Gatherer Societies},
  author = {Smith, Eric Alden and Codding, Brian F.},
  date = {2021-03-30},
  journaltitle = {Proceedings of the National Academy of Sciences},
  shortjournal = {Proc. Natl. Acad. Sci. U.S.A.},
  volume = {118},
