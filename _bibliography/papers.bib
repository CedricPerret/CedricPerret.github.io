@article{bissonnette_Coalitions_2015,
  title = {Coalitions in Theory and Reality: A Review of Pertinent Variables and Processes},
  shorttitle = {Coalitions in Theory and Reality},
  author = {Bissonnette, Annie and Perry, Susan and Barrett, Louise and Mitani, John C. and Flinn, Mark and Gavrilets, Sergey and family=Waal, given=Frans B.M., prefix=de, useprefix=true},
